<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest ExtensionBundleId="com.linkmanager" ExtensionBundleVersion="1.0.0" Version="7.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <Extension Id="com.linkmanager.panel" Version="1.0.0"/>
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="IDSN" Version="13.1"/>
    </HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="7.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.linkmanager.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./client/index.html</MainPath>
          <ScriptPath>./host/index.jsx</ScriptPath>
          <CEFCommandLine> 
            <Parameter>--enable-nodejs</Parameter> 
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
          <StartOn>
            <Event>com.linkmanager.focus</Event>
          </StartOn>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Link Manager</Menu>
          <Geometry>
            <Size>
              <Height>400</Height>
              <Width>600</Width>
            </Size>
          </Geometry>
          <Icons>
            <Icon Type="Normal">./host/icons/IconDark.png</Icon>
            <Icon Type="RollOver">./host/icons/IconDark.png</Icon>
            <Icon Type="DarkNormal">./host/icons/IconDark.png</Icon>
            <Icon Type="DarkRollOver">./host/icons/IconDark.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>